{
  "name": "markocupic/gmk-newsteaser-bundle",
  "type": "contao-bundle",
  "description": "Contao 4 Gmk Newsteaser Bundle",
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "Marko Cupic",
      "homepage": "https://github.com/markocupic"
    }
  ],
  "require": {
    "php": "^5.6|^7.0",
    "symfony/framework-bundle": "^3.3",
    "contao/core-bundle": "^4.4"
  },
  "autoload": {
    "psr-4": {
      "Markocupic\\GmkNewsteaserBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config",
      "src/Resources/contao/dca",
      "src/Resources/contao/languages",
      "src/Resources/contao/templates"
    ]
  },
  "extra": {
    "contao-manager-plugin": "Markocupic\\GmkNewsteaserBundle\\ContaoManager\\Plugin"
  }
}



