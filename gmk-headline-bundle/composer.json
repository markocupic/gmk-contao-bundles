{
  "name": "markocupic/gmk-headline-bundle",
  "type": "contao-bundle",
  "description": "Contao 4 Gmk Headline Bundle",
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "Marko Cupic",
      "homepage": "https://github.com/markocupic"
    }
  ],
  "version": "1.0.0",
  "require": {
    "php": "^7.0",
    "contao/core-bundle": "^4.9"
  },
  "autoload": {
    "psr-4": {
      "Markocupic\\GmkHeadlineBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config",
      "src/Resources/contao/dca",
      "src/Resources/contao/languages",
      "src/Resources/contao/templates"
    ]
  },
  "extra": {
    "contao-manager-plugin": "Markocupic\\GmkHeadlineBundle\\ContaoManager\\Plugin"
  }
}



